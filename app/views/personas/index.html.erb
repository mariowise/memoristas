<h1>Listing personas</h1>

<table>
  <thead>
    <tr>
      <th>Run persona</th>
      <th>Nombre persona</th>
      <th>Apellido persona</th>
      <th>Email persona</th>
      <th>Fono persona</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @personas.each do |persona| %>
      <tr>
        <td><%= persona.run_persona %></td>
        <td><%= persona.nombre_persona %></td>
        <td><%= persona.apellido_persona %></td>
        <td><%= persona.email_persona %></td>
        <td><%= persona.fono_persona %></td>
        <td><%= link_to 'Show', persona %></td>
        <td><%= link_to 'Edit', edit_persona_path(persona) %></td>
        <td><%= link_to 'Destroy', persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Persona', new_persona_path %>
